if [ -f ./lcg/checklcg ]
then
  ./insertlib lcg ./checksprng "Checking Default C++ interface:" "./lcg/check.clcg < ./lcg/lcg.data 2> /dev/null" ./lcg/check.clcg
  ./insertlib lcg ./checksprng "Checking Simple C++ interface:" "./lcg/check_sim.clcg < ./lcg/lcg.data 2> /dev/null" ./lcg/check_sim.clcg

./insertlib lcg ./checksprng "Checking Default FORTRAN interface:" "./lcg/check.flcg < ./lcg/lcg.data 2> /dev/null" ./lcg/check.flcg
./insertlib lcg ./checksprng "Checking Simple FORTRAN interface:" "./lcg/check_sim.flcg < ./lcg/lcg.data 2> /dev/null" ./lcg/check_sim.flcg
fi

if [ -f ./lcg64/checklcg64 ]
then
  ./insertlib lcg64 ./checksprng "Checking Default C++ interface:" "./lcg64/check.clcg64 < ./lcg64/lcg64.data 2> /dev/null" ./lcg64/check.clcg64
  ./insertlib lcg64 ./checksprng "Checking Simple C++ interface:" "./lcg64/check_sim.clcg64 < ./lcg64/lcg64.data 2> /dev/null" ./lcg64/check_sim.clcg64

./insertlib lcg64 ./checksprng "Checking Default FORTRAN interface:" "./lcg64/check.flcg64 < ./lcg64/lcg64.data 2> /dev/null" ./lcg64/check.flcg64
./insertlib lcg64 ./checksprng "Checking Simple FORTRAN interface:" "./lcg64/check_sim.flcg64 < ./lcg64/lcg64.data 2> /dev/null" ./lcg64/check_sim.flcg64
fi

